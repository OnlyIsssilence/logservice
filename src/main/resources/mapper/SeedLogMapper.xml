<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.onlyisssilence.muya.dao.SeedLogMapper">

    <resultMap id="BaseResultMap" type="com.onlyisssilence.muya.entity.SeedLog">
        <id column="log_id" property="logId" jdbcType="VARCHAR"/>
        <result column="city_code" property="cityCode" jdbcType="VARCHAR"/>
        <result column="version" property="version" jdbcType="TINYINT"/>
        <result column="uid" property="uid" jdbcType="VARCHAR"/>
        <result column="seed_time" property="seedTime" jdbcType="TIMESTAMP"/>
        <result column="create_name" property="createName" jdbcType="TIMESTAMP"/>
    </resultMap>

    <sql id="Base_Column_List">
        log_id,city_code,version,uid,seed_time,create_time
    </sql>


    <!-- 根据logId查询单条log语句 -->
    <select id="queryByLogId" resultMap="BaseResultMap" parameterType="long">
        <!-- 具体的sql -->
        SELECT
        <include refid="Base_Column_List"/>
        FROM
        seed_log
        WHERE
        log_id = #{logId}
    </select>

    <!-- 查询所有的log数据 -->
    <select id="queryAll" resultMap="BaseResultMap">
        select
        <include refid="Base_Column_List"/>
        from seed_log
    </select>

    <insert id="insert" parameterType="com.onlyisssilence.muya.entity.SeedLog">
        insert into seed_log (city_code,version,uid,seed_time)
        values (#{cityCode,jdbcType=VARCHAR},
                #{version,jdbcType=VARCHAR},
                #{uid,jdbcType=VARCHAR},
                #{seedTime,jdbcType=TIMESTAMP})
    </insert>

</mapper>

















